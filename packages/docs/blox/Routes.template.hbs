import { Route, Routes, Navigate } from "react-router-dom";
{{#each components}}
import { {{this}}Demo } from './{{this}}Demo'
{{/each}}

export const BloxRoutes = ({dependencies}) => (
  <Routes>
    <Route path="/" element={ <Navigate to="/Welcome" /> } />
    {{#each components}}
    <Route path="{{this}}" element={<{{this}}Demo dependencies={dependencies} />} />
    {{/each}}
  </Routes>
);