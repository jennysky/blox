import { Route, Routes } from "react-router-dom";
import { Welcome } from "../Welcome";
{{#each components}}
import { {{this}}Demo } from './{{this}}Demo'
{{/each}}

export const BloxRoutes = ({dependencies}) => (
  <Routes>
    <Route exact path="/" element={<Welcome />} />
    {{#each components}}
    <Route exact path="{{this}}" element={<{{this}}Demo dependencies={dependencies} />} />
    {{/each}}
  </Routes>
);